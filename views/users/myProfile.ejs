<div class="account-dashboard">
  <div class="container">
    <div class="row">
      <div class="col-sm-12 col-md-3 col-lg-3">
        <!-- Nav tabs -->
        <div class="dashboard_tab_button" data-aos="fade-up" data-aos-delay="0">
          <ul role="tablist" class="nav flex-column dashboard-list">
            <li><a href="#dashboard" data-bs-toggle="tab"
              class="nav-link btn btn-block btn-md btn-black-default-hover active">My Profile</a>
            </li>
            <li> <a href="#orders" data-bs-toggle="tab"
              class="nav-link btn btn-block btn-md btn-black-default-hover">Orders</a></li>
            <li><a href="#address" data-bs-toggle="tab"
              class="nav-link btn btn-block btn-md btn-black-default-hover">Addresses</a></li>
            <li><a href="/logout"
              class="nav-link btn btn-block btn-md btn-black-default-hover">logout</a></li>
          </ul>
        </div>
      </div>
      <div class="col-sm-12 col-md-9 col-lg-9">
        <!-- Tab panes -->
        <div class="tab-content dashboard_content" data-aos="fade-up" data-aos-delay="200">
          <div class="tab-pane fade show active" id="dashboard">
            <div class="container profile-container">
              <div class="row">
                <div class="col-md-4 text-center">
                  <img src="https://via.placeholder.com/150" alt="Profile Picture" class="profile-picture">
                  <br><br>
                  <div>
                    your referal code is <%= user.referCode %>
                    <button></button>
                  </div>
                </div>
                <div class="col-md-8">
                  <% if(typeof user !== 'undefined') { %>
                  <h2 class="mb-4">User Profile</h2>
                  <div style="display: flex; align-items: center;" class="mb-3">
                    <strong>Name:</strong><%= user.name %>
                    <div class="mb-3" id="newUserName" style="display: none;">
                      <input type="text" placeholder="enter new name" id="userNameNew" class="form-control" style="margin-top: 10px;">
                    </div>
                  </div>                  
                  <div class="mb-3">
                    <strong>Email:</strong> <%= user.email %>
                  </div>
                  <div class="mb-3" style="display: flex; align-items: center;">
                    <strong>Phone:</strong> <%= user.mobile %>
                    <div class="mb-3" id="newUserMobile" style="display: none;">
                      <input type="number" placeholder="enter new number" class="form-control" id="userMobileNew" style="margin-top: 10px;">
                    </div>
                  </div>
                  <div class="mb-3">
                    <strong>Joined On:</strong> <%= date %>
                  </div>
                  <button class="btn btn-primary" id="editProfileButton" onclick="editProfile()">Edit Profile</button>
                  <button class="btn btn-primary" id="saveButton" style="display: none;" onclick="saveProfileChanges()">save</button>
                  <button><a href="/loadChangePassword">change password</a></button>
                  <% } %>
                </div>
                <div style="display: none;">
                  <div class="d-flex">
                    <div class="toast-body">
                      Hello, world! This is a toast message.
                    </div>
                    <button type="button" class="btn-close me-2 m-auto" data-bs-dismiss="toast" aria-label="Close"></button>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="tab-pane fade" id="orders">
            <% if (typeof orders !== 'undefined' && orders.length > 0) { %>
              <h4>Orders</h4>
              <div class="table_page table-responsive">
                <table>
                  <thead>
                    <tr>
                      <th>Order</th>
                      <th>Date</th>
                      <th>Status</th>
                      <th>Total</th>
                      <th>Actions</th>
                    </tr>
                  </thead>
                  <% orders.reverse().forEach((order) => { %>
                    <tbody>
                      <tr>
                        <td><%= order.orderId %></td>
                        <td><%= order.date.toString().substring(0,16) %></td>
                        <td><span style="background-color: white;" class="success"><%= order.orderStatus %></span></td>
                        <td>$<%= order.subtotal %> for <%= order.items.length %> items</td>
                        <form action="/orderDetails" method="post">
                          <input type="hidden" name="orderId" value="<%= order.orderId %>">
                          <td><a href="" class="view"><button type="submit" style="background-color: transparent;">view</button></a></td>
                        </form>
                      </tr>
                    </tbody>
                  <% }) %>
                </table>
              </div>
            <% } else { %>
              <h1>Nothing to show</h1>
            <% } %>
          </div>
                  
          <div class="tab-pane fade" id="downloads">
            <h4>Downloads</h4>
            <div class="table_page table-responsive">
              <table>
                <thead>
                  <tr>
                    <th>Product</th>
                    <th>Downloads</th>
                    <th>Expires</th>
                    <th>Download</th>
                  </tr>
                </thead>
                <tbody>
                  <tr>
                    <td>Shopnovilla - Free Real Estate PSD Template</td>
                    <td>May 10, 2018</td>
                    <td><span class="danger">Expired</span></td>
                    <td><a href="#" class="view">Click Here To Download Your File</a></td>
                  </tr>
                  <tr>
                    <td>Organic - ecommerce html template</td>
                    <td>Sep 11, 2018</td>
                    <td>Never</td>
                    <td><a href="#" class="view">Click Here To Download Your File</a></td>
                  </tr>
                </tbody>
              </table>
            </div>
          </div>
          <div class="tab-pane" id="address">
            <p>The following addresses will be used on the checkout page by default.</p>
            <h5 class="billing-address">Billing address</h5>
            <a href="#" class="view" data-bs-toggle="modal" data-bs-target="#addAddressModal">Add</a>
            <address>
              <div>
                <div>
                  <form action="deleteAddress" method="post">
                    <div id="primaryAddress">
                      <% if (typeof user !== 'undefined' && user.address.length > 0) { %>
                        <div style="border: double rgb(67, 214, 224);">
                          <div class="col-3">
                            Address:<br>
                          </div>
                          <div class="d-flex flex-direction-horizontal justify-content-between">
                            <% const latestAddress = user.address.length > 0
                            ? user.address.reduce((prev, current) => {
                                return (current.makeDefault > prev.makeDefault) ? current : prev;
                              })
                            : null; %>
                            <p><strong><%= latestAddress.userName %></strong></p>
                            <div col-6>
                              <p><strong>House Name :</strong><%= latestAddress.houseName %></p>
                              <p><strong>Landmark   :</strong><%= latestAddress.landmark %></p>
                              <p><strong>postOffice :</strong><%= latestAddress.postOffice %></p>
                              <p><strong>District   :</strong><%= latestAddress.district %></p>
                            </div>
                            <div col-6>
                              <p><strong>State      :</strong><%= latestAddress.state %></p>
                              <p><strong>Country    :</strong><%= latestAddress.country %></p>
                              <p><strong>PIN        :</strong><%= latestAddress.pin %></p>
                              <p><strong>Contact No.:</strong><%= latestAddress.contactNumber %></p>
                              <input type="hidden" name="makeDefault" value="<%= latestAddress.makeDefault %>">
                            </div>
                          </div>
                        </div>
                        <div class="d-grid gap-2 d-md-flex justify-content-md-end">
                          <button><a href="#" class="view" data-bs-toggle="modal" data-bs-target="#editAddressModal">Edit</a></button>
                          <button type="submit">Delete</button>
                        </div>
                      <% } else { %>
                        <p>No address available</p>
                      <% } %>
                    </div>
                  </form>                            
                </div>
                <div>
                  <div>
                    <% if ( typeof user !== 'undefined' && user.address.length > 0 ) { %>
                      <div class="col-3">
                        Address: <br>
                      </div>
                      <% user.address.forEach((address)=>{ %>
                      <form id="myForm" method="post" action="/makeDefaultAddress">
                        <div style="border: 1px solid gray;">
                          <p><strong><%= address.userName %></strong></p>
                          <p><%= address.houseName %> , <%= address.landmark %> , <%= address.postOffice %> , <%= address.district %></p>
                          <p><st><%= address.state %> , <%= address.country %> , <%= address.pin %> , <%= address.contactNumber %></p>
                          <input type="hidden" name="makeDefault" value="<%= address.makeDefault %>">
                          <button class="btn" onclick="makedefaultaddress()">Make Default</button>
                          <button class="btn" onclick="deleteaddress()">Delete</button>
                        </div>
                      </form>
                      <% }) %>
                    <% } %>
                  </div>
                </div>
              </div>
            </address>
            <!-- add address Modal -->
            <div class="modal fade" id="addAddressModal" tabindex="-1" aria-labelledby="addAddressModalLabel" aria-hidden="true">
              <div class="modal-dialog modal-dialog-centered modal-sm">
                <div class="modal-content">
                  <div class="modal-header">
                    <h5 class="modal-title" id="addAddressModalLabel">Add Address</h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                  </div>
                  <div class="modal-body">
                    <!-- Two-column layout -->
                    <form action="addaddress" method="post">
                      <div class="row">
                        <div class="col-md-6">
                          <!-- Column 1 content -->
                          <div class="mb-3">
                            <label for="userName" class="form-label">Name</label>
                            <input type="text" class="form-control" name="userName" id="userName">
                          </div>
                          <div class="mb-3">
                            <label for="houseName" class="form-label">House Name</label>
                            <input type="text" class="form-control" name="houseName" id="houseName" required>
                          </div>
                          <div class="mb-3">
                            <label for="postOffice" class="form-label">Post Office</label>
                            <input type="text" class="form-control" id="postOffice" name="postOffice" required>
                          </div>
                          <div class="mb-3">
                            <label for="landmark" class="form-label">Landmark</label>
                            <input type="text" class="form-control" id="landmark" name="landmark" required>
                          </div>
                          <div class="mb-3">
                            <label for="district" class="form-label">District</label>
                            <input type="text" class="form-control" id="district" name="district" required>
                          </div>
                        </div>
                        <div class="col-md-6">
                          <!-- Column 2 content -->
                          <div class="mb-3">
                            <label for="state" class="form-label">State</label>
                            <input type="text" class="form-control" name="state" id="state" required>
                          </div>
                          <div class="mb-3">
                            <label for="country" class="form-label">Country</label>
                            <input type="text" class="form-control" id="country" name="country" required>
                          </div>
                          <div class="mb-3">
                            <label for="pin" class="form-label">PIN</label>
                            <input type="text" class="form-control" id="pin" name="pin" required>
                          </div>
                          <div class="mb-3">
                            <label for="contactNumber" class="form-label">Contact Number</label>
                            <input type="text" class="form-control" id="contactNumber" name="contactNumber" required>
                          </div>
                        </div>
                      </div>
                      <div style="display: none;">
                        <input type="date" value="">
                      </div>
                      <!-- End of two-column layout -->
                      <button type="submit" class="btn btn-primary">Save Address</button>
                    </form>
                  </div>
                </div>
              </div>
            </div>
            <!-- edit address Modal -->
            <div class="modal fade" id="editAddressModal" tabindex="-1" aria-labelledby="editAddressModalLabel" aria-hidden="true">
              <div class="modal-dialog modal-dialog-centered modal-sm">
                <div class="modal-content">
                  <div class="modal-header">
                    <h5 class="modal-title" id="editAddressModalLabel">Edit Address</h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                  </div>
                  <div class="modal-body">
                    <!-- Two-column layout -->
                    <form action="editaddress" method="post">
                      <div class="row">
                        <div class="col-md-6">
                          <!-- Column 1 content -->
                          <div class="mb-3">
                            <label for="userName" class="form-label">Name</label>
                            <input type="text" class="form-control" name="userName" id="userName">
                          </div>
                          <div class="mb-3">
                            <label for="houseName" class="form-label">House Name</label>
                            <input type="text" class="form-control" name="houseName" id="houseName">
                          </div>
                          <div class="mb-3">
                            <label for="postOffice" class="form-label">Post Office</label>
                            <input type="text" class="form-control" id="postOffice" name="postOffice">
                          </div>
                          <div class="mb-3">
                            <label for="landmark" class="form-label">Landmark</label>
                            <input type="text" class="form-control" id="landmark" name="landmark">
                          </div>
                          <div class="mb-3">
                            <label for="district" class="form-label">District</label>
                            <input type="text" class="form-control" id="district" name="district">
                          </div>
                        </div>
                        <div class="col-md-6">
                          <!-- Column 2 content -->
                          <div class="mb-3">
                            <label for="state" class="form-label">State</label>
                            <input type="text" class="form-control" name="state" id="state">
                          </div>
                          <div class="mb-3">
                            <label for="country" class="form-label">Country</label>
                            <input type="text" class="form-control" id="country" name="country">
                          </div>
                          <div class="mb-3">
                            <label for="pin" class="form-label">PIN</label>
                            <input type="text" class="form-control" id="pin" name="pin">
                          </div>
                          <div class="mb-3">
                            <label for="contactNumber" class="form-label">Contact Number</label>
                            <input type="text" class="form-control" id="contactNumber" name="contactNumber">
                          </div>
                        </div>
                      </div>
                      <!-- End of two-column layout -->
                      <button type="submit" class="btn btn-primary">Update Address</button>
                    </form>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
